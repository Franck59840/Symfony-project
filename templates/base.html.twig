<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link href="https://fonts.cdnfonts.com/css/marvel" rel="stylesheet">
        <link rel="icon" type="images/png" href="{{ asset('build/images/logo.png') }}" />  

        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {{ encore_entry_link_tags('headerCSS') }}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('header') }}
        {% block javascripts %}
        {% endblock %}
    </head>
    <body>
        {% include 'Components/_header.html.twig' %}
        {% block body %}{% endblock %}
        <img src="{{ asset('build/path_to_image.png') }}" alt=""/> 
    </body>
</html>

